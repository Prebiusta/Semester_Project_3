@model System.Collections.Generic.List<WebCoreMVC.NET.Models.UserStory>

@{
    ViewData["Title"] = "Product Backlog";
}

<!DOCTYPE html>

<html>
<head>
    <title>Product Backlog</title>
    <link rel="stylesheet" type="text/css" href="/css/custom.css">
</head>
<body>
<h1>User Stories</h1>
<div class="row">
    <div class="structure">
        @foreach (var item in Model)
        {
            <div class="userstory">
                <h1>@item.description</h1>
            </div>
        }
    </div>
</div>
</body>
</html>